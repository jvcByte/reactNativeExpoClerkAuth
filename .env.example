# Thirdweb Configuration
NEXT_PUBLIC_THIRDWEB_CLIENT_ID=your_thirdweb_client_id_here

# Application Configuration
NEXT_PUBLIC_APP_NAME="DApp"
NEXT_PUBLIC_APP_DESCRIPTION="A production-ready template"
NEXT_PUBLIC_APP_URL=http://localhost:3000

# SEO Configuration
NEXT_PUBLIC_SEO_TITLE="DApp"
NEXT_PUBLIC_SEO_DESCRIPTION="A production-ready template"
NEXT_PUBLIC_SEO_KEYWORDS="thirdweb, blockchain, dapp, web3"
NEXT_PUBLIC_SEO_AUTHOR="Thirdweb Team"
NEXT_PUBLIC_OG_IMAGE="/og-image.png"

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_ERROR_REPORTING=true
NEXT_PUBLIC_ENABLE_PWA=false

# API Configuration
NEXT_PUBLIC_API_BASE_URL=/api
NEXT_PUBLIC_API_TIMEOUT=10000

# Environment
NODE_ENV=development
